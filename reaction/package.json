{
  "name": "xmtp-content-type-reaction",
  "version": "1.0.0",
  "description": "An XMTP content type to support emoji reactions to messages",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "umd:main": "dist/umd/index.js",
  "browser": "dist/umd/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:types",
    "build:cjs": "node config/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node config/cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:umd": "node config/cleanup umd && webpack --config config/webpack.config.js",
    "build:types": "node config/cleanup types && tsc -p config/tsconfig.types.json",
    "package": "npm pack",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@noble/secp256k1": "^1.7.1",
    "@xmtp/proto": "^3.15.0",
    "@xmtp/xmtp-js": "^7.12.0"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "eslint-plugin-prettier": "^4.2.1",
    "ethers": "^6.0.8",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "semantic-release": "^20.1.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
